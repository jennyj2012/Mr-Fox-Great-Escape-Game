//\uc0\u8232 //Cindy Juarez and Jenny Juarez\u8232 //3.9.10\u8232 //The Fox Game\u8232 //Code taken from lecture notes, homework examples, as well as processing.org\u8232 //To play this game press space to jump, "." to go forward, "," to go back.\u8232 //The object of the game is to have your fox land inside the balloon by jumping across moving rocks\u8232 //Final Project\u8232 \u8232 //initialize variables\u8232 int num = 10;\u8232 Clouds[] clouds = new Clouds[3];\u8232 Rocks[] rocks = new Rocks[num];\u8232 Balloon balloon;\u8232 Fox fox;\u8232 Cliff cliff;\u8232 \u8232 //create clouds, rocks, fox, cliff, and balloon\u8232 void setup()\{\u8232     size(700,700);\u8232     smooth();\u8232     noStroke();\u8232     for(int i = 0; i < clouds.length; i++)\{\u8232         clouds[i] = new Clouds();\u8232     \}\u8232     for(int i = 0; i < rocks.length; i++)\{\u8232         rocks[i] = new Rocks();\u8232     \}\u8232     balloon = new Balloon();\u8232     fox = new Fox();\u8232     cliff = new Cliff();\u8232 \}\u8232 \u8232 //draw and move variables\u8232 void draw()\{\u8232     background(102,204,255);\u8232     for(int i = 0; i < clouds.length; i++) \{\u8232         clouds[i].clouds1();\u8232         clouds[i].clouds2();\u8232         clouds[i].update();\u8232     \}\u8232     for(int i = 0; i < rocks.length; i++) \{\u8232         rocks[i].draw();\u8232         rocks[i].update();\u8232     \}\u8232 \u8232     cliff.draw();\u8232     fox.foxDraw();\u8232     fox.foxUpdate();\u8232     balloon.draw();\u8232     balloon.update();\u8232 \u8232     //if fox is in balloon, print "You Won"\u8232     if(fox.wins > 0)\{\u8232         PFont font;\u8232         font = loadFont("Arial-Black-60.vlw");\u8232         textFont(font);\u8232         text("You Won!", 2*width/4, height/4);\u8232     \}\u8232 \}\u8232 \u8232 //listen for keyPressed actions to move fox\u8232 void keyPressed()\{\u8232     fox.keypressed();\u8232 \}\
